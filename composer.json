{
  "name": "artisan-cloud/saas-framework",
  "description": "This package is a saas build framework  for developer via laravel framework",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "michael hu",
      "email": "hbj148@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "ArtisanCloud\\SaaSFramework\\": "src/"
    },
    "files": [
      "src/Supports/helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanCloud\\SaaSFramework\\Providers\\FrameworkServiceProvider"
      ],
      "aliases": [
        {
          "checkHeader": "ArtisanCloud\\SaaSFramework\\Http\\Middleware\\CheckHeader::class"
        },
        {
          "checkUser": "ArtisanCloud\\SaaSFramework\\Http\\Middleware\\CheckUser::class"
        },
        {
          "checkClientHavingUser": "ArtisanCloud\\SaaSFrameworkHttp\\Middleware\\CheckClientHavingUser::class"
        }
      ]
    }
  }
}

